{
	"metadata": [
		{
			"src": [
				{
					"files": ["**/*.csproj"],
					"src": "../src/src"
				}
			],
			"dest": "api"
		}
	],
	"build": {
		"content": [
			{
				"files": ["articles/**.md", "articles/toc.yml", "api/**.md", "api/**.yml", "toc.yml", "*.md"]
			}
		],
		"resource": [
			{
				"files": ["favicon.png", "articles/images/**"]
			}
		],
		"dest": "_site",
		"globalMetadata": {
			"_appFooter": "godspeed",
			"_appFaviconPath": "favicon.png",
			"_enableSearch": true
		  },
		"globalMetadataFiles": [],
		"fileMetadataFiles": [],
		"template": ["default"],
		"postProcessors": [],
		"markdownEngineName": "markdig",
		"noLangKeyword": false,
		"keepFileLink": false,
		"cleanupCacheHistory": false,
		"disableGitFeatures": false
	}
}
